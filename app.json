{
  "expo": {
    "name": "todo cm",
    "slug": "todo-cm",
    "version": "1.2.2",
    "githubUrl": "https://github.com/Cristian-F-M/todo-app-v2",
    "orientation": "portrait",
    "icon": "./assets/logo-cm-white.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "todo-cm",
    "platforms": [
      "android"
    ],
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#111827"
    },
    "splash": {
      "image": "./assets/logo-cm-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-cm-small.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.morales.todocm"
    },
    "web": {
      "favicon": "./assets/logo-cm-small.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo-cm-white.png"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8148ba5-8662-4e8d-8b2c-ed66367e4e7d"
      }
    }
  }
}