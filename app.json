{
  "expo": {
    "name": "todo-cm",
    "slug": "todo-cm",
    "owner": "cm-team",
    "version": "1.0.0",
    "githubUrl": "https://github.com/Cristian-F-M/todo-app-v2",
    "orientation": "portrait",
    "icon": "./assets/logo-cm-white.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "todo-cm",
    "splash": {
      "image": "./assets/logo-cm-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-cm-small.png",
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "favicon": "./assets/logo-cm-small.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ]
  }
}