{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@icons/*": [
        "./icons/*"
      ],
      "@context/*": [
        "./context/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@constants/*": [
        "./constants/*"
      ]
    },
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
  },
}